[user]
	name = Stefany
	email = scampanhoni@gmail.com

[core]
	editor = nvim

[alias]
    	co = checkout
    	ci = commit
	br = branch
	df = diff
	log = log --graph --pretty=format:'%Cred%h%Creset -%C(auto)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit

[color]
    ui = true

[color "branch"]
    current = magenta
    local = cyan
    remote = green

[color "diff"]
    meta = yellow
    frag = magenta
    old = red
    new = green

[color "status"]
    added = cyan
    changed = magenta
    untracked = yellow
